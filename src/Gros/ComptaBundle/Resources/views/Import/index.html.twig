{% extends "GrosComptaBundle::layout.html.twig" %}

{% block title %}
    Data Import - {{ parent() }}
{% endblock %}

{% block sub_title %}
    Data Import
{% endblock %}

{% block header_javascripts %}
{{ parent() }}
<script type="text/javascript">
</script>
{% endblock %}

{% block sidebar %}
{{ parent() }}
{% endblock %}

{% form_theme form with 'GrosComptaBundle:Default:fields.html.twig' %}

{% block groscompta_body %}
<div class="row">
    <div class="span9">
        <form class="form-horizontal" method="post" {{ form_enctype(form) }}>
            {{ form_widget(form) }}
            <input type="submit" class="btn btn-primary" value="Upload CSV"/>
        </form>
    </div>
</div>
{% endblock %}

{% block body_javascripts %}
{{ parent() }}
{% endblock %}
